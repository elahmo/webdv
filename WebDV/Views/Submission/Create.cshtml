@model WebDV.Models.Submission
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Upload a submission</title>
</head>
<body>
    <h1>Upload a Submission</h1>
    <form enctype="multipart/form-data" method="post" action="/Submission/Upload/@Model">
