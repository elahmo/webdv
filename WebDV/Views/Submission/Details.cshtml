@model WebDV.Models.Submission[]

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <div>
        <h4>Submission</h4>
        <hr />        
        @if (Model.Length == 0)
        {
            <h1>No Submission Found</h1>
        }
        else
        {
            <h1>Submission details for @Model[0].submissionID</h1>
            foreach (WebDV.Models.Submission sub in Model)
            {
                    if (sub.submissionData != null)
                    {
                    <iframe srcdoc="@System.Text.Encoding.UTF8.GetString(sub.submissionData)"></iframe>
                }
            }
        }
    </div>
</body>
</html>
